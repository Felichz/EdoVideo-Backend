{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "DB_USER": "@edovideo-db-user",
        "DB_PASSWORD": "@edovideo-db-password",
        "DB_HOST": "@edovideo-db-host",
        "DB_NAME": "@edovideo-db-name"
    }
}